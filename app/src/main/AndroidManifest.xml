<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chenby.androidclient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.chenby.SplashActivity"
            android:theme="@style/Account_Base.App.Theme.NoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chenby.sign.MainActivity"
            android:theme="@style/Base.App.Theme.NoTitle" />
        <activity
            android:name="com.example.chenby.sign.RegisterActivity"
            android:theme="@style/Base.App.Theme.NoTitle" />
        <activity android:name=".ChatListActivity"></activity>
    </application>

</manifest>